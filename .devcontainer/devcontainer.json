{
  "name": "jlg.io",
  "build": {
    "args": {
      "VARIANT": "16-bullseye"
    },
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "streetsidesoftware.code-spell-checker"
      ]
    }
  },
  "forwardPorts": [3000],
  "postCreateCommand": "pnpm install",
  "remoteUser": "node",
  "settings": {
    "npm.packageManager": "pnpm"
  }
}
