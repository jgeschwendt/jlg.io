{
  "name": "jlg.io",
  "version": "0.1.0",
  "description": "jlg.io",
  "main": "n/a",
  "repository": "git@github.com:jgeschwendt/jlg.io.git",
  "author": "jlg",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "BABEL_ENV=production NODE_ENV=production webpack",
    "generate:elm-interop": "elm-typescript-interop",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "lint-fix": "eslint --ext .ts --ext .tsx --fix src/",
    "npm-check": "ncu",
    "npm-check-upgrade": "ncu -u",
    "profile": "BABEL_ENV=production NODE_ENV=production webpack --profile --json > dist/stats-production.json && webpack-bundle-analyzer dist/stats-production.json dist --mode static --no-open --report dist/report-production.html",
    "profile:dev": "BABEL_ENV=development NODE_ENV=development webpack --profile --json > dist/stats-development.json && webpack-bundle-analyzer dist/stats-development.json dist --mode static --no-open --report dist/report-development.html",
    "start": "BABEL_ENV=development NODE_ENV=development webpack-dev-server",
    "start:dashboard": "BABEL_ENV=development NODE_ENV=development WEBPACK_DASHBOARD=true webpack-dashboard -- webpack-dev-server",
    "tsc": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@jlg/styled-components": "git+https://github.com/jgeschwendt/jlg-styled-components.git",
    "core-js": "3.6.4",
    "elm": "0.19.1-3",
    "lodash": "4.17.15",
    "polished": "3.4.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-ga": "2.7.0",
    "react-helmet": "5.2.1",
    "react-router": "5.1.2",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "react-transition-group": "4.3.0",
    "styled-components": "4.4.1",
    "styled-reboot": "4.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router": "5.1.4",
    "@types/react-router-config": "5.0.1",
    "@types/react-router-dom": "5.1.3",
    "@types/react-transition-group": "4.2.3",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-styled-components": "1.10.7",
    "circular-dependency-plugin": "5.2.0",
    "elm-analyse": "0.16.5",
    "elm-format": "0.8.2",
    "elm-typescript-interop": "0.0.15",
    "elm-webpack-loader": "6.0.1",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "npm-check-updates": "4.0.1",
    "prop-types": "15.7.2",
    "serverless": "1.62.0",
    "serverless-finch": "2.5.2",
    "terser-webpack-plugin": "2.3.4",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dashboard": "3.2.0",
    "webpack-dev-server": "3.10.1"
  }
}
