{
  "name": "jlg.io",
  "version": "0.1.0",
  "description": "jlg.io",
  "main": "n/a",
  "repository": "git@github.com:jgeschwendt/jlg.io.git",
  "author": "jlg",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "BABEL_ENV=production NODE_ENV=production webpack",
    "generate:elm-interop": "elm-typescript-interop",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "lint-fix": "eslint --ext .ts --ext .tsx --fix src/",
    "profile": "BABEL_ENV=production NODE_ENV=production webpack --profile --json > dist/stats-production.json && webpack-bundle-analyzer dist/stats-production.json dist --mode static --no-open --report dist/report-production.html",
    "profile:dev": "BABEL_ENV=development NODE_ENV=development webpack --profile --json > dist/stats-development.json && webpack-bundle-analyzer dist/stats-development.json dist --mode static --no-open --report dist/report-development.html",
    "start": "BABEL_ENV=development NODE_ENV=development webpack-dev-server",
    "start:dashboard": "BABEL_ENV=development NODE_ENV=development WEBPACK_DASHBOARD=true webpack-dashboard -- webpack-dev-server",
    "tsc": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "core-js": "3.8.2",
    "elm": "0.19.1-3",
    "framer": "1.2.12",
    "framer-motion": "2.9.5",
    "lodash": "4.17.20",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-ga": "3.3.0",
    "react-helmet": "6.1.0",
    "react-router": "5.2.0",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.1",
    "styled-reboot": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@types/react": "16.14.0",
    "@types/react-dom": "16.9.9",
    "@types/react-router": "5.1.9",
    "@types/react-router-config": "5.0.2",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.12.0",
    "circular-dependency-plugin": "5.2.2",
    "elm-analyse": "0.16.5",
    "elm-format": "0.8.4",
    "elm-typescript-interop": "0.0.17",
    "elm-webpack-loader": "7.0.1",
    "eslint": "7.17.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "4.5.1",
    "jest": "26.6.3",
    "serverless": "1.83.0",
    "serverless-cloudfront-invalidate": "1.5.0",
    "serverless-s3-sync": "1.15.0",
    "terser-webpack-plugin": "4.2.3",
    "typescript": "4.1.3",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-dashboard": "3.2.1",
    "webpack-dev-server": "3.11.1"
  }
}
