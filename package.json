{
  "name": "jlg.io",
  "version": "0.0.0",
  "description": "web profile application",
  "author": "joshua@geschwendt.com",
  "license": "UNLICENSED",
  "repository": "git@github.com:jgeschwendt/jlg.io.git",
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "packageManager": "pnpm@10.3.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "esbuild",
      "sharp"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "singleQuote": true
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "geist": "1.3.1",
    "http-status-codes": "2.3.0",
    "motion": "12.4.2",
    "next": "15.2.0-canary.53",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "serialize-error": "12.0.0",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.20.0",
    "@next/eslint-plugin-next": "15.2.0-canary.53",
    "@tailwindcss/postcss": "4.0.6",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-unicorn": "56.0.1",
    "globals": "15.14.0",
    "postcss": "8.5.2",
    "prettier": "3.5.0",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "4.0.6",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.0",
    "vercel": "41.1.0"
  },
  "scripts": {
    "build": "next build",
    "cspell": "pnpm dlx cspell **/*.{md,mjs,ts,tsx}",
    "dev": "next dev --turbopack",
    "favicon": "pnpm dlx svg-to-ico public/jg.svg src/app/favicon.ico",
    "lint": "next lint",
    "ncu": "pnpm dlx npm-check-updates -u && pnpm up && pnpm install",
    "start": "next start"
  }
}
