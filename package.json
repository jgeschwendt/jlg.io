{
  "name": "jlg.io",
  "version": "0.1.0",
  "description": "jlg.io",
  "main": "n/a",
  "repository": "git@github.com:jgeschwendt/jlg.io.git",
  "author": "jlg",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "BABEL_ENV=production NODE_ENV=production webpack",
    "generate:elm-interop": "elm-typescript-interop",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "lint-fix": "eslint --ext .ts --ext .tsx --fix src/",
    "npm-check": "ncu",
    "npm-check-upgrade": "ncu -u",
    "profile": "BABEL_ENV=production NODE_ENV=production webpack --profile --json > dist/stats-production.json && webpack-bundle-analyzer dist/stats-production.json dist --mode static --no-open --report dist/report-production.html",
    "profile:dev": "BABEL_ENV=development NODE_ENV=development webpack --profile --json > dist/stats-development.json && webpack-bundle-analyzer dist/stats-development.json dist --mode static --no-open --report dist/report-development.html",
    "start": "BABEL_ENV=development NODE_ENV=development webpack-dev-server",
    "start:dashboard": "BABEL_ENV=development NODE_ENV=development WEBPACK_DASHBOARD=true webpack-dashboard -- webpack-dev-server",
    "tsc": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@jlg/styled-components": "git+https://github.com/jgeschwendt/jlg-styled-components.git",
    "core-js": "3.6.5",
    "elm": "0.19.1-3",
    "lodash": "4.17.15",
    "polished": "3.6.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "2.7.0",
    "react-helmet": "5.2.1",
    "react-router": "5.2.0",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.2.0",
    "react-transition-group": "4.4.1",
    "styled-components": "5.1.0",
    "styled-reboot": "4.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router": "5.1.7",
    "@types/react-router-config": "5.0.1",
    "@types/react-router-dom": "5.1.5",
    "@types/react-transition-group": "4.2.4",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "circular-dependency-plugin": "5.2.0",
    "elm-analyse": "0.16.5",
    "elm-format": "0.8.3",
    "elm-typescript-interop": "0.0.17",
    "elm-webpack-loader": "6.0.1",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "25.5.3",
    "npm-check-updates": "4.1.2",
    "serverless": "1.71.1",
    "serverless-cloudfront-invalidate": "1.4.0",
    "serverless-s3-sync": "1.14.1",
    "terser-webpack-plugin": "2.3.6",
    "typescript": "3.9.2",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dashboard": "3.2.0",
    "webpack-dev-server": "3.11.0"
  }
}
