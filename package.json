    "babel-plugin-styled-components": "1.11.1",
    "circular-dependency-plugin": "5.2.0",
    "eslint": "7.11.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
