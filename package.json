{
  "name": "jlg.io",
  "version": "0.1.0",
  "description": "jlg.io",
  "main": "n/a",
  "repository": "git@github.com:jgeschwendt/jlg.io.git",
  "author": "jlg",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "BABEL_ENV=production NODE_ENV=production webpack",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "lint-fix": "eslint --ext .ts --ext .tsx --fix src/",
    "npm-check": "ncu",
    "npm-check-upgrade": "ncu -u",
    "profile": "BABEL_ENV=production NODE_ENV=production webpack --profile --json > dist/stats-production.json && webpack-bundle-analyzer dist/stats-production.json dist --mode static --no-open --report dist/report-production.html",
    "profile:dev": "BABEL_ENV=development NODE_ENV=development webpack --profile --json > dist/stats-development.json && webpack-bundle-analyzer dist/stats-development.json dist --mode static --no-open --report dist/report-development.html",
    "start": "BABEL_ENV=development NODE_ENV=development webpack-dev-server",
    "start:dashboard": "BABEL_ENV=development NODE_ENV=development WEBPACK_DASHBOARD=true webpack-dashboard -- webpack-dev-server",
    "tsc": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "polished": "3.4.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-ga": "2.6.0",
    "react-helmet": "5.2.1",
    "react-router": "5.0.1",
    "react-router-config": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-transition-group": "4.2.1",
    "styled-components": "4.3.2",
    "styled-transition-group": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "1.10.6",
    "circular-dependency-plugin": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "eslint": "6.1.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "jest": "24.8.0",
    "npm-check-updates": "3.1.20",
    "prop-types": "15.7.2",
    "serverless": "1.48.4",
    "serverless-finch": "2.4.2",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "4.37.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.6",
    "webpack-dashboard": "3.0.7",
    "webpack-dev-server": "3.7.2"
  }
}
