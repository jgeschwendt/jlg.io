    "react-helmet": "6.1.0",
    "react-router": "5.2.0",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.0",
    "styled-reboot": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
